(library
 (name imagelib)
 (kind normal)
 (synopsis "Image library")
 (archives (byte imagelib.cma) (native imagelib.cmxa))
 (plugins (byte imagelib.cma) (native imagelib.cmxs))
 (native_archives imagelib.a)
 (requires stdlib-shims checkseum.ocaml decompress.zl)
 (modes byte native)
 (modules
  (unwrapped
   (module
    (obj_name image)
    (visibility public)
    (source (path Image) (intf (path image.mli)) (impl (path image.ml))))
   (module
    (obj_name imageBMP)
    (visibility public)
    (source (path ImageBMP) (impl (path imageBMP.ml))))
   (module
    (obj_name imageGIF)
    (visibility public)
    (source
     (path ImageGIF)
     (intf (path imageGIF.mli))
     (impl (path imageGIF.ml))))
   (module
    (obj_name imageJPG)
    (visibility public)
    (source (path ImageJPG) (impl (path imageJPG.ml))))
   (module
    (obj_name imageLib)
    (visibility public)
    (source
     (path ImageLib)
     (intf (path imageLib.mli))
     (impl (path imageLib.ml))))
   (module
    (obj_name imagePNG)
    (visibility public)
    (source (path ImagePNG) (impl (path imagePNG.ml))))
   (module
    (obj_name imagePPM)
    (visibility public)
    (source (path ImagePPM) (impl (path imagePPM.ml))))
   (module
