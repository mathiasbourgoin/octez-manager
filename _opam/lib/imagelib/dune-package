(library
 (name imagelib.unix)
 (kind normal)
 (synopsis "Image library with unix bindings")
 (archives (byte unix/imagelib_unix.cma) (native unix/imagelib_unix.cmxa))
 (plugins (byte unix/imagelib_unix.cma) (native unix/imagelib_unix.cmxs))
 (native_archives unix/imagelib_unix.a)
 (requires imagelib unix)
 (modes byte native)
 (modules
  (unwrapped
   (module
    (obj_name imageLib_unix)
    (visibility public)
    (source
     (path ImageLib_unix)
     (intf (path unix/imageLib_unix.mli))
     (impl (path unix/imageLib_unix.ml))))
   (module
    (obj_name imageUtil_unix)
    (visibility public)
    (source (path ImageUtil_unix) (impl (path unix/imageUtil_unix.ml)))))))
